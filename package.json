{
  "name": "sevnstack",
  "version": "0.0.1",
  "description": "An interactive CLI to scaffold, build and deploy full-stack web apps with Nuxt, Vuetify and Express.",
  "author": "Basedwon <basedwon@protonmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basedwon/sevn.git"
  },
  "bugs": {
    "url": "https://github.com/basedwon/sevn/issues"
  },
  "homepage": "https://github.com/basedwon/sevn#readme",
  "main": "lib/sevnstack.js",
  "bin": {
    "sevn": "./lib/sevnstack.js"
  },
  "nodemonConfig": {
    "ignore": [
      "tmp",
      "templates"
    ],
    "watch": [
      "**"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "scripts": {
    "dev": "nodemon . -- create foo",
    "docs": "jsdoc2md \"lib/**/*.js\" > docs/api.md",
    "update": "npm run update:prod && npm run update:dev",
    "update:prod": "npm i -S basd@latest",
    "update:dev": "npm i -D @basd/testr@latest",
    "test": "testr test"
  },
  "dependencies": {
    "basd": "^1.0.16",
    "cli-color": "^2.0.3",
    "cli-spinners": "^2.9.2",
    "commander": "^11.1.0",
    "handlebars": "^4.7.8",
    "prompts": "^2.4.2",
    "spinnies": "^0.5.1"
  },
  "devDependencies": {
    "@basd/testr": "^0.1.2",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0"
  },
  "keywords": [
    "full-stack",
    "web-application",
    "scaffolding",
    "nuxt",
    "vuetify",
    "express",
    "cli",
    "nodejs",
    "project-generator",
    "ssr",
    "static-site-generation",
    "docker",
    "web-development",
    "server-side-rendering",
    "javascript",
    "development-tools",
    "websockets",
    "pwa",
    "authentication",
    "monolithic",
    "decoupled",
    "https",
    "api",
    "frontend",
    "backend"
  ]
}
